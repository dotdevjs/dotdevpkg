{
  "npmScope": "dotdevpkg",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e", "compodoc"]
      }
    }
  },
  "projects": {
    "nest-crud": {
      "tags": ["nest"]
    },
    "nest-core": {
      "tags": ["nest"]
    },
    "nest-typeorm": {
      "tags": ["nest"]
    },
    "react-inversify": {
      "tags": ["react"]
    },
    "nest-crud-typeorm": {
      "tags": ["nest"]
    },
    "nest-crud-request": {
      "tags": ["nest"]
    },
    "nest-console": {
      "tags": ["nest"]
    },
    "docs": {
      "tags": []
    }
  },
  "workspaceLayout": {
    "appsDir": "e2e",
    "libsDir": "packages"
  }
}
