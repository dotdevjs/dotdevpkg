{
  "name": "nestpkg",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "nx": "nx",
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "lint": "nx workspace-lint && nx lint",
    "e2e": "nx e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "lerna": "lerna",
    "lerna:patch": "lerna version patch && git add lerna.json && git commit -m \"(lerna): Update version\" --allow-empty",
    "lerna:patch:publish": "npm run affected:test -- --all --parallel --maxParallel=5 && npm run lerna:patch && npm run affected:build -- --all --parallel --maxParallel=5 && lerna publish from-package --contents dist",
    "test:cov": "nx affected:test --all --parallel --maxParallel=5 --code-coverage --coverageDirectory=tmp/coverage",
    "compodoc": "nx run docs:compodoc --serve --port=3080 --watch=true --hideGenerator=true",
    "workspace-generator": "nx workspace-generator"
  },
  "private": true,
  "dependencies": {
    "@nestjs/common": "7.5.5",
    "@nestjs/core": "7.5.5",
    "@nestjs/typeorm": "7.1.5",
    "@nestjsx/crud": "4.6.2",
    "@nestjsx/crud-typeorm": "4.6.2",
    "class-transformer": "0.3.1",
    "class-validator": "0.13.1",
    "inversify": "5.0.1",
    "lodash": "4.17.20",
    "monkeypatch": "1.0.0",
    "nestjs-command": "1.5.3",
    "reflect-metadata": "0.1.13",
    "slugify": "1.4.6",
    "typeorm": "0.2.29",
    "uuid": "8.3.1"
  },
  "devDependencies": {
    "@babel/core": "7.12.9",
    "@babel/preset-env": "7.12.7",
    "@babel/preset-typescript": "7.12.7",
    "@compodoc/compodoc": "1.1.11",
    "@nestjs/testing": "7.5.5",
    "@nrwl/cli": "11.3.0",
    "@nrwl/eslint-plugin-nx": "11.3.0",
    "@nrwl/jest": "11.3.0",
    "@nrwl/nest": "11.3.0",
    "@nrwl/node": "11.3.0",
    "@nrwl/nx-plugin": "11.3.0",
    "@nrwl/tao": "11.3.0",
    "@nrwl/workspace": "11.3.0",
    "@twittwer/compodoc": "1.5.1",
    "@types/lodash": "4.14.167",
    "@types/node": "12.12.38",
    "@types/uuid": "8.3.0",
    "@typescript-eslint/eslint-plugin": "4.9.0",
    "@typescript-eslint/parser": "4.9.0",
    "babel-jest": "26.6.3",
    "dotenv": "8.2.0",
    "eslint": "7.14.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-prettier": "3.3.1",
    "husky": "4.3.0",
    "jest": "26.6.3",
    "lerna": "3.22.1",
    "lint-staged": "10.5.3",
    "prettier": "2.2.1",
    "rimraf": "3.0.2
    "ts-jest": "26.4.4",
    "sqlite3": "5.0.1",
    "ts-node": "9.1.1",
    "tslint": "6.1.3",
    "typescript": "4.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "packages/**/*.{ts,json}": [
      "nx format:write --all --skip-nx-cache"
    ]
  }
}